    "def square_cm(func):\n",
    "    def convert(r):\n",
    "        area = func(r)\n",
    "        return area * (100**2)\n",
    "    \n",
    "    return convert"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 13,
   "metadata": {},
   "outputs": [],
   "source": [
    "@square_cm\n",
    "def circle_area_m(r):\n",
    "    pi = 3.1416\n",
    "    \n",
    "    return pi * (r**2)"
   ]
  },
